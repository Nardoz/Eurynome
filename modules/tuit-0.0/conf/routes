# This file defines all module routes (Higher priority routes first)
#
# import these routes in the main app as :
# *     /                       module:tuit
#
# ~~~~

# GET     /?                      Module.index
GET     /	                    tuit.TuitController.index
GET     /?                      tuit.TuitController.index
GET     /postDM                 tuit.TuitController.postDM
GET		/timeline				tuit.TuitController.timeline
GET		/signin					tuit.TuitController.signin
GET		/signout				tuit.TuitController.signout
GET		/callback				tuit.TuitController.callback
GET		/test					tuit.TuitController.test




